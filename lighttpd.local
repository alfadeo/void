server.port = 8080
server.document-root = "/home/ch/src/void"
#server.document-root = "/www"
server.modules = ( "mod_rewrite" )
mimetype.assign = (
  ".html" => "text/html", 
  ".jpg" => "image/jpeg",
  ".png" => "image/png",
  ".svg" => "image/svg+xml", 
	".css" => "text/css",
)
url.rewrite-once = (
  "^/info.html$" => "info.html",
  "^.+html$" => "index.html",
  "^/$" => "index.html"
)
